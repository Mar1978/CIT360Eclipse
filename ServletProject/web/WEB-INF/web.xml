<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>getFormValues</servlet-name>
        <servlet-class>getFormValues</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>S2_getFormValues</servlet-name>
        <servlet-class>Introduction.S2_getFormValues</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>S1_hello</servlet-name>
        <servlet-class>Introduction.S1_hello</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewServlet</servlet-name>
        <servlet-class>Introduction.NewServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>getFormValues</servlet-name>
        <url-pattern>/getFormValues</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>S2_getFormValues</servlet-name>
        <url-pattern>/S2_getFormValues</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>S1_hello</servlet-name>
        <url-pattern>/S1_hello</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewServlet</servlet-name>
        <url-pattern>/NewServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
